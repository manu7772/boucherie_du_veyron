{% extends 'sitesiteBundle:bases:base_' ~ cssjsloads|default('simple')|lower ~ '.html.twig' %}

{% block body %}
	{% block header %}
	<header id="header"><!--header-->
		{% block headerTop %}
			{{ include("sitesiteBundle:blocks:headerTop.html.twig") }}
		{% endblock headerTop %}
		{% block headerMiddle %}
			{{ include('sitesiteBundle:blocks:headerMiddle.html.twig') }}
		{% endblock headerMiddle %}
	</header><!--/header-->
	{% endblock header %}

	{% block mainContainer %}
	{% endblock mainContainer %}

	{% block footers %}
	<footer id="footer"><!--Footer-->
		{% block footerTop %}
			{{ render(controller("sitesiteBundle:Default:footerTop")) }}
		{% endblock footerTop %}
		{% block footerBottom %}
			{{ include("sitesiteBundle:blocks:footerBottom.html.twig") }}
		{% endblock footerBottom %}
	</footer><!--/Footer-->
	{% endblock footers %}

{% endblock body %}
