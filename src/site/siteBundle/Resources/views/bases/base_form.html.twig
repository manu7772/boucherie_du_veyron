{% extends 'sitesiteBundle:bases:base_simple.html.twig' %}

{% block css %}
	{% stylesheets filter='cssrewrite, lessphp, ?yui_css'
	'bundles/sitegraphics/bdv/css/bootstrap.min.css'
	'bundles/sitegraphics/bdv/css/font-awesome.min.css'
	'bundles/laboadmin/framework_inspinia/css/plugins/toastr/toastr.min.css'
	'bundles/sitegraphics/framework_inspinia/font-awesome/less/font-awesome.less'
	'bundles/sitegraphics/bdv/css/animate.css'
	'bundles/sitegraphics/bdv/css/main2.css'
	'bundles/sitegraphics/bdv/css/responsive.css'
	'bundles/laboadmin/framework_inspinia/cropper-master/dist/cropper.css'
	'bundles/sitegraphics/fancyapps-fancyBox/source/jquery.fancybox.css'
	 %}
	<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
	{% endstylesheets %}
	{% block ielt9 %}
	<!--[if lt IE 9]>
	<script src="js/html5shiv.js"></script>
	<script src="js/respond.min.js"></script>
	<![endif]-->
	{% endblock ielt9 %}
{% endblock css %}

{% block js %}
	{% javascripts filter='?yui_js'
	'bundles/sitegraphics/bdv/js/jquery.js'
	'bundles/sitegraphics/bdv/js/bootstrap.min.js'
	'bundles/sitegraphics/bdv/js/jquery.scrollUp.min.js'
	'bundles/laboadmin/framework_inspinia/js/plugins/toastr/toastr.min.js'
	'bundles/sitegraphics/bdv/js/main.js'
	'bundles/sitegraphics/bdv/js/specifics.js'
	'bundles/laboadmin/framework_inspinia/cropper-master/dist/cropper.js'
	'bundles/laboadmin/js/inspinia-cropper.js'
	'bundles/laboadmin/js/inspinia-data-messages.js'
	'bundles/sitegraphics/fancyapps-fancyBox/source/jquery.fancybox.js'
    'bundles/laboadmin/js/ajaxlive.js'
	 %}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
	<script type="text/javascript">
	jQuery(document).ready(function($) {
		{% block end_javascripts %}
		{% endblock end_javascripts %}
	});
	</script>
{% endblock js %}