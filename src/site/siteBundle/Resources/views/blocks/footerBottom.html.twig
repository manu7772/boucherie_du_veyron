{% if app.session.get('sitedata') != null %}
{% if app.session.get('sitedata').boutiques|default(null) != null %}
<div class="footer-bottom">
	<div class="container">
		<div class="row">
			{% set boutique = app.session.get('sitedata').boutiques.0|default(null) %}
			<p class="pull-left">{{ 'footer.rights'|trans|capitalize }} Â© {{ annee() }} - {{ 'site'|trans }} {{ app.session.get('sitedata').nom }}{% if boutique.adresse is not null %} - {{ boutique.adresse.adresse }} - {{ boutique.adresse.cp|default('') }} {{ boutique.adresse.ville|default('') }}{% endif %}</p>
			<p class="pull-right">{{ 'footer.realisation'|trans({'%name%': 'Aequation-Webdesign.fr'})|capitalize }} </p>
		</div>
	</div>
</div>
{% endif %}
